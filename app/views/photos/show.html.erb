<div class="card photo-form-card">
  <div class="card-header">
    <h2>Фото <%= @photo.id %></h2>
  </div>
  <div class="card-body">

    <p id="notice"><%= notice %></p>

    <p>
      <strong>Description:</strong>
      <%= @photo.description %>
    </p>

    <%= link_to 'Edit', edit_photo_path(@photo) %> |
    <%= link_to 'Back', photos_path %>

  </div>
</div>
